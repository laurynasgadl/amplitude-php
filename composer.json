{
  "name": "laurynasgadl/amplitude-php",
  "type": "library",
  "description": "PHP Amplitude client package",
  "keywords": [
    "php",
    "amplitude",
    "client"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Laurynas Gadliauskas",
      "homepage": "https://github.com/laurynasgadl"
    }
  ],
  "require": {
    "php": ">=7.4",
    "psr/log": "^1.0",
    "guzzlehttp/guzzle": "^7.0.1",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Luur\\Amplitude\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Luur\\Amplitude\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  }
}
